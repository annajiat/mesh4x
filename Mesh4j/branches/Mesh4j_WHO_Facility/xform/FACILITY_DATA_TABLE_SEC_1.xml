<?xml version="1.0" encoding="UTF-8"?>

<h:html xmlns:h="http://www.w3.org/1999/xhtml" 
		xmlns="http://www.w3.org/2002/xforms" 
		xmlns:ev="http://www.w3.org/2001/xml-events" 
		xmlns:xsd="http://www.w3.org/2001/XMLSchema" 
		xmlns:jr="http://openrosa.org/javarosa">  
  <h:head> 
    <h:title>FACILITY_DATA_TABLE_SEC_1</h:title>  
    <model> 
      <instance> 
        <FACILITY_DATA_TABLE_SEC_1>
        
        <facility_id/>
          <q101_interviewstart_time/>  
          <q102_is_landphone_available/>
          <q103_is_cellphone_available/>
          <q104_is_sw_radio_available/>  
          
          <q105_is_phone_or_radio_available_around/>
          
          <q106_is_compter_available/> 
          
          <q107_is_email_or_internet_available/> 
          
          <q108_is_connected_to_central_elec_grid/>
          
          <q109_is_other_power_supply_available/>
          
          <q109_is_other_power_supply_available_other/>
          
          <q110_is_generator_functinal/>  
          
          <q111_was_elec_continuously_available/>
          
          <q112_src_of_water_for_hw/>
          
          <q112_src_of_water_for_hw_other/>
            
          <q113_is_src_of_water_for_hw_available_nearby/>
          
          <q114_is_standalone_lab/>  
          
          <q115_is_standalone_pharmacy/> 
          
          <q116_does_provide_impatient_care/>
          
          <q117_is_beds_avaliable/>
             
          <q117_beds_count/> 
          
          <q118_past_admission_count/> 
          
          <q119_is_beds_avaliable_for_oo/> 
           
          <q119_beds_count_for_oo/>
          
          <q120_is_beds_avaliable_for_db/>   
          
          <q120_beds_count_for_db/>   
           
          <q121_deivery_count/>
          
          <q122_is_beds_avaliable_for_maternity/> 
          
          <q122_beds_count_for_maternity/>
          
          <q123_is_waiting_area_available_for_ip/>
          <sec1_control_field/>
          <is_respondant_agrees/>
            
        </FACILITY_DATA_TABLE_SEC_1> 
      </instance>  
      
      <bind id="pq101_interviewstart_time" nodeset="/FACILITY_DATA_TABLE_SEC_1/q101_interviewstart_time" type="xsd:date" jr:preload="date" jr:preloadParams="today"/>
      <bind id="pq121_deivery_count" nodeset="/FACILITY_DATA_TABLE_SEC_1/q121_deivery_count" type="xsd:long"/>  
      <bind id="pq112_src_of_water_for_hw_other" nodeset="/FACILITY_DATA_TABLE_SEC_1/q112_src_of_water_for_hw_other" type="xsd:string"/>  
      <bind id="pfacility_id" nodeset="/FACILITY_DATA_TABLE_SEC_1/facility_id" type="xsd:string"/>  
      <bind id="pq120_beds_count_for_db" nodeset="/FACILITY_DATA_TABLE_SEC_1/q120_beds_count_for_db" type="xsd:long"/>  
      <bind id="pq109_is_other_power_supply_available_other" nodeset="/FACILITY_DATA_TABLE_SEC_1/q109_is_other_power_supply_available_other" type="xsd:string"/>  
      <bind id="pq117_beds_count" nodeset="/FACILITY_DATA_TABLE_SEC_1/q117_beds_count" type="xsd:long"/>  
      <bind id="pq119_beds_count_for_oo" nodeset="/FACILITY_DATA_TABLE_SEC_1/q119_beds_count_for_oo" type="xsd:long"/>  
      <bind id="pq118_past_admission_count" nodeset="/FACILITY_DATA_TABLE_SEC_1/q118_past_admission_count" type="xsd:long"/>  
      <bind id="pq122_beds_count_for_maternity" nodeset="/FACILITY_DATA_TABLE_SEC_1/q122_beds_count_for_maternity" type="xsd:long"/>
       
    </model> 
  </h:head>  
  <h:body> 
  
    <select1 ref="/FACILITY_DATA_TABLE_SEC_1/is_respondant_agrees"> 
      <label>Does Respondent Agrees to be Interviewed?</label>  
      <item>
        <label>Yes</label>
        <value>true</value>
      </item>  
      <item>
        <label>No</label>
        <value>false</value>
      </item> 
    </select1>
    
    <input bind="pq101_interviewstart_time">
      <label>101.Interview Start time:</label>
    </input>
  
    <select1 ref="/FACILITY_DATA_TABLE_SEC_1/q102_is_landphone_available"> 
      <label>102.Does the facility have a functioning land line telephone?</label>  
      <item>
        <label>Yes</label>
        <value>true</value>
      </item>  
      <item>
        <label>No</label>
        <value>false</value>
      </item> 
    </select1>  
    
    
     <select1 ref="/FACILITY_DATA_TABLE_SEC_1/q103_is_cellphone_available"> 
        <label>103.Does the facility have functioning cellular telephones(either private or supported by the facility)?</label>  
      <item>
        <label>Yes</label>
        <value>true</value>
      </item>  
      <item>
        <label>No</label>
        <value>false</value>
      </item> 
    </select1>  
    
     <select1 ref="/FACILITY_DATA_TABLE_SEC_1/q104_is_sw_radio_available"> 
      <label>104.Does the facility have a functioning short-wave radio for radio calls?</label>  
      <item>
        <label>Yes</label>
        <value>true</value>
      </item>  
      <item>
        <label>No</label>
        <value>false</value>
      </item> 
    </select1>
    
    <input bind="pq105_is_phone_or_radio_available_around">
      <label>105.Is there a phone or shortwave radio within less than 15 minutes walking distance from the facility that staff can use in an emergency?</label>
       <item>
        <label>YES,AT ALL TIMES</label>
        <value>YES,AT ALL TIMES</value>
      </item>  
      <item>
        <label>YES,BUT NOT AT ALL TIMES</label>
        <value>YES,BUT NOT AT ALL TIMES</value>
      </item>
      <item>
        <label>NO</label>
        <value>NO</value>
      </item>
    </input>  
    
    
     
    
     <select1 ref="/FACILITY_DATA_TABLE_SEC_1/pq106_is_compter_available"> 
      <label>106.Does the facility have a computer? </label>  
      <item>
        <label>YES,FUNCTIONING</label>
        <value>YES,FUNCTIONING</value>
      </item>  
      <item>
        <label>YES,NOT FUNCTIONING</label>
        <value>YES,NOT FUNCTIONING</value>
      </item>
      <item>
        <label>NO</label>
        <value>NO</value>
      </item> 
    </select1>
    
    
    <select1 ref="/FACILITY_DATA_TABLE_SEC_1/q107_is_email_or_internet_available"> 
      <label>Is there access to email/internet within the facility?</label>  
      <item>
        <label>Yes</label>
        <value>true</value>
      </item>  
      <item>
        <label>No</label>
        <value>false</value>
      </item> 
    </select1>
    
    
    <select1 ref="/FACILITY_DATA_TABLE_SEC_1/q108_is_connected_to_central_elec_grid"> 
      <label>Is this facility connected to the central supply (national or local) electricity grid?</label>  
      <item>
        <label>Yes</label>
        <value>true</value>
      </item>  
      <item>
        <label>No</label>
        <value>false</value>
      </item> 
    </select1>
    
    
    <select1 ref="/FACILITY_DATA_TABLE_SEC_1/pq109_is_other_power_supply_available"> 
      <label>Does the facility have other sources of electricity like generator or solar supply?</label>  
      <item>
        <label>GENERATOR</label>
        <value>GENERATOR</value>
      </item>  
      <item>
        <label>SOLAR SUPPLY</label>
        <value>SOLAR SUPPLY</value>
      </item>
      <item>
        <label>OTHER</label>
        <value>OTHER</value>
      </item> 
    </select1>
    
     <input bind="pq109_is_other_power_supply_available_other">
      <label>Specify if other:</label>
    </input>  
    
    
    <select1 ref="/FACILITY_DATA_TABLE_SEC_1/pq110_is_generator_functinal"> 
      <label>Is the generator functional?</label>  
      <item>
        <label>YES, FUNCTIONAL WITH FUEL</label>
        <value>GENERATOR</value>
      </item>  
      <item>
        <label>YES, FUNCTIONAL, NO FUEL</label>
        <value>SOLAR SUPPLY</value>
      </item>
      
      <item>
        <label>NOT FUNCTIONAL</label>
        <value>OTHER</value>
      </item>
      
      <item>
        <label>DON'T KNOW</label>
        <value>OTHER</value>
      </item>
       
    </select1>
    
    
    <select1 ref="/FACILITY_DATA_TABLE_SEC_1/q111_was_elec_continuously_available"> 
      <label>During the past week was electric power continuously available in this facility at the times the facility was open for services?</label>  
      <item>
        <label>Yes</label>
        <value>true</value>
      </item>  
      <item>
        <label>No</label>
        <value>false</value>
      </item> 
    </select1>
    
    
    <select1 ref="/FACILITY_DATA_TABLE_SEC_1/q112_src_of_water_for_hw"> 
        <label>Commonly used source of water for hand washing for the facility at this time?</label>  
      <item>
        <label>PIPED INTO FACILITY</label>
        <value>1</value>
      </item>  
      <item>
        <label>PIPED ONTO FACILITY GROUNDS</label>
        <value>2</value>
      </item>
      <item>
        <label>PUBLIC TAP/STANDPIPE</label>
        <value>3</value>
      </item>
      <item>
        <label>TUBEWELL/BOREHOLE</label>
        <value>4</value>
      </item>
       <item>
        <label>PROTECTED DUG WELL</label>
        <value>5</value>
      </item>
      <item>
        <label>UNPROTECTED DUG WELL</label>
        <value>6</value>
      </item>
      <item>
        <label>PROTECTED SPRING</label>
        <value>7</value>
      </item>
      <item>
        <label>UNPROTECTED SPRING</label>
        <value>8</value>
      </item>
      <item>
        <label>RAINWATER</label>
        <value>9</value>
      </item>
      <item>
        <label>BOTTLED WATER</label>
        <value>10</value>
      </item>
      <item>
        <label>CART W/SMALL TANK/DRUM</label>
        <value>11</value>
      </item>
      <item>
        <label>TANKER TRUCK</label>
        <value>12</value>
      </item>
      <item>
        <label>SURFACE WATER (RIVER, DAM, POND)</label>
        <value>13</value>
      </item>
      <item>
        <label>OTHER(SPECIFY)</label>
        <value>14</value>
      </item>
      <item>
        <label>DON'T KNOW</label>
        <value>15</value>
      </item>
      <item>
        <label>NO WATER SOURCE</label>
        <value>16</value>
      </item>
    </select1>
    
    
     
     <input bind="pq112_src_of_water_for_hw_other">
      <label>Specify if other:</label>
    </input>  
    
    
    <select1 ref="/FACILITY_DATA_TABLE_SEC_1/q113_is_src_of_water_for_hw_available_nearby"> 
      <label> Is a water outlet from this source available within 500 meters of the facility?</label>  
      <item>
        <label>Yes</label>
        <value>true</value>
      </item>  
      <item>
        <label>No</label>
        <value>false</value>
      </item> 
    </select1>
    
    
    <select1 ref="/FACILITY_DATA_TABLE_SEC_1/q114_is_standalone_lab"> 
      <label>Is the facility a stand-alone laboratory?</label>  
      <item>
        <label>Yes</label>
        <value>true</value>
      </item>  
      <item>
        <label>No</label>
        <value>false</value>
      </item> 
    </select1>
    
    
    <select1 ref="/FACILITY_DATA_TABLE_SEC_1/q115_is_standalone_pharmacy"> 
      <label>Is the facility a stand-alone pharmacy or drugstore?</label>  
      <item>
        <label>Yes</label>
        <value>true</value>
      </item>  
      <item>
        <label>No</label>
        <value>false</value>
      </item> 
    </select1>
    
    
    <select1 ref="/FACILITY_DATA_TABLE_SEC_1/q116_does_provide_impatient_care"> 
      <label>Does this facility routinely provide inpatient care?</label>  
      <item>
        <label>Yes</label>
        <value>true</value>
      </item>  
      <item>
        <label>No</label>
        <value>false</value>
      </item> 
    </select1>
    
    
    <select1 ref="/FACILITY_DATA_TABLE_SEC_1/q117_is_beds_avaliable"> 
      <label>Does this facility have beds for routine care?</label>  
      <item>
        <label>Yes</label>
        <value>true</value>
      </item>  
      <item>
        <label>No</label>
        <value>false</value>
      </item> 
    </select1>
    
    <input bind="pq117_beds_count">
      <label>How many beds?</label>
    </input>  
    
    <input bind="pq118_past_admission_count">
      <label>How many in-patients(In last calendar month) were admitted to this facility?</label>
    </input>  
    
    
    <select1 ref="/FACILITY_DATA_TABLE_SEC_1/q119_is_beds_avaliable_for_oo"> 
      <label>Does this facility have beds for overnight observation?</label>  
      <item>
        <label>Yes</label>
        <value>true</value>
      </item>  
      <item>
        <label>No</label>
        <value>false</value>
      </item> 
    </select1>
    
    <input bind="pq119_beds_count_for_oo">
      <label>How many beds?</label>
    </input>  
    
    
    <select1 ref="/FACILITY_DATA_TABLE_SEC_1/q120_is_beds_avaliable_for_db"> 
      <label>Does this facility have beds just to deliver babies?</label>  
      <item>
        <label>Yes</label>
        <value>true</value>
      </item>  
      <item>
        <label>No</label>
        <value>false</value>
      </item> 
    </select1>
    
    <input bind="pq120_beds_count_for_db">
      <label>How many delivery beds?</label>
    </input>  
    
    
    <input bind="pq121_deivery_count">
      <label>During (the last completed calendar month) how many deliveries took place in this facility?</label>
    </input>  
      
     
    <select1 ref="/FACILITY_DATA_TABLE_SEC_1/q122_is_beds_avaliable_for_maternity"> 
      <label> Does this facility have maternity beds, i.e., beds to rest after delivery?</label>  
      <item>
        <label>Yes</label>
        <value>true</value>
      </item>  
      <item>
        <label>No</label>
        <value>false</value>
      </item> 
    </select1>  
    
    <input bind="pq122_beds_count_for_maternity">
      <label>How many delivery beds?</label>
    </input>  
    
    <select1 ref="/FACILITY_DATA_TABLE_SEC_1/q123_is_waiting_area_available_for_ip"> 
      <label>Does this facility have a separate waiting area for infectious patients?</label>  
      <item>
        <label>Yes</label>
        <value>true</value>
      </item>  
      <item>
        <label>No</label>
        <value>false</value>
      </item> 
    </select1>
    
     
  </h:body> 
</h:html>
